; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[addons]

resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/color_arrays=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="MyceliumGuardians"
run/main_scene="res://Scenes/Main/Main.tscn"
config/features=PackedStringArray("4.3")
boot_splash/show_image=false

[audio]

driver/mix_rate=48000

[autoload]

Global="*res://Classes/Global.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/shadowed_variable=0
gdscript/warnings/integer_division=0
gdscript/warnings/narrowing_conversion=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/size/initial_screen.editor=1
window/size/initial_position_type.editor=0

[dotnet]

project/assembly_name="MyceliumGuardians"

[editor]

movie_writer/movie_file="/home/magic/Videos/movie_writer/before_gfx_rebalance_02.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/script-tabs/plugin.cfg")

[gui]

timers/tooltip_delay_sec=0.01
theme/custom="res://Assets/default_theme_v3_1080p.tres"
theme/custom_font="res://Assets/ui/fonts/Kumbh_Sans/KumbhSans-VariableFont_YOPQ,wght.ttf"

